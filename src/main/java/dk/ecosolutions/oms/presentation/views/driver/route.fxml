<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1000.0" stylesheets="@../../css/styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.ecosolutions.oms.application.controllers.driver.RouteController">
    <children>
        <BorderPane fx:id="routesIndex" prefHeight="400.0" prefWidth="600.0" styleClass="container">
            <top>
                <HBox alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="580.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label styleClass="title" text="Routes" />
                        <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="505.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Button mnemonicParsing="false" onAction="#startLocation" text="Start Location">
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                    <styleClass>
                                        <String fx:value="btn" />
                                        <String fx:value="btn-success" />
                                    </styleClass>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                    <BorderPane.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </BorderPane.margin>
                </HBox>
            </top>
         <center>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="551.0" prefWidth="492.0">
                     <children>
                        <Label text="Pickup" />
                              <TableView fx:id="pickup" prefHeight="597.0" prefWidth="492.0">
                                  <columns>
                                      <TableColumn fx:id="pickupName" prefWidth="491.0" text="Location" />
                                  </columns>
                              </TableView>
                     </children>
                  </VBox>
                  <VBox prefHeight="551.0" prefWidth="489.0">
                     <children>
                        <Label text="Delivery" />
                        <TableView fx:id="delivery" prefHeight="534.0" prefWidth="495.0">
                          <columns>
                            <TableColumn fx:id="deliveryName" prefWidth="488.0" text="Location" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </center>
        </BorderPane>
        <BorderPane fx:id="routesView" prefHeight="200.0" prefWidth="200.0" styleClass="container" visible="false">
            <top>
                <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="980.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label styleClass="title" text="Orders" />
                        <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="505.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Button mnemonicParsing="false" onAction="#startOrder" text="Start Order">
                                    <styleClass>
                                        <String fx:value="btn" />
                                        <String fx:value="btn-success" />
                                    </styleClass>
                                </Button>
                                <Button fx:id="index" mnemonicParsing="false" onAction="#changeDisplay" text="Back">
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                    <styleClass>
                                        <String fx:value="btn" />
                                        <String fx:value="btn-primary" />
                                    </styleClass>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <BorderPane.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </BorderPane.margin>
                </HBox>
            </top>
            <center>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <TableView fx:id="orders" prefHeight="400.0" prefWidth="960.0">
                            <columns>
                                <TableColumn fx:id="orderId" prefWidth="195.0" text="Id" />
                            </columns>
                        </TableView>
                    </children>
                </VBox>
            </center>
        </BorderPane>
        <BorderPane fx:id="routesProcess" prefHeight="200.0" prefWidth="200.0" styleClass="container" visible="false">
            <top>
                <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="980.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label styleClass="title" text="Items" />
                        <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="505.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Button mnemonicParsing="false" onAction="#scan" text="Scan">
                                    <styleClass>
                                        <String fx:value="btn" />
                                        <String fx:value="btn-success" />
                                    </styleClass>
                                </Button>
                                <Button fx:id="view" mnemonicParsing="false" onAction="#changeDisplay" text="Back">
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                    <styleClass>
                                        <String fx:value="btn" />
                                        <String fx:value="btn-primary" />
                                    </styleClass>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <BorderPane.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </BorderPane.margin>
                </HBox>
            </top>
            <center>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <TableView fx:id="items" prefHeight="200.0" prefWidth="200.0">
                            <columns>
                                <TableColumn fx:id="itemId" editable="false" prefWidth="485.0" sortable="false" text="ID" />
                                <TableColumn fx:id="itemCloth" editable="false" prefWidth="493.0" sortable="false" text="Cloth" />
                        <TableColumn fx:id="itemQuantity" editable="false" prefWidth="493.0" sortable="false" text="Quantity" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </VBox>
            </center>
        </BorderPane>
    </children>
</StackPane>
