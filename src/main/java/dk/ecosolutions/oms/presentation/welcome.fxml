<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="auth" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.251"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.ecosolutions.oms.application.AuthController">
    <center>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="300.0" layoutY="164.0" text="Email"/>
                <TextField fx:id="email" layoutX="300.0" layoutY="187.0" prefHeight="25.0" prefWidth="200.0"/>
                <Label layoutX="300.0" layoutY="216.0" text="Password"/>
                <TextField fx:id="password" layoutX="300.0" layoutY="233.0" prefHeight="25.0" prefWidth="200.0"/>
                <Button layoutX="350.0" layoutY="271.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0"
                        prefWidth="100.0" text="Login"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
